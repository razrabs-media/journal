{
  "name": "raz.media.web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "update-schema": "graphql-codegen -c ./codegen.download.yml",
    "dev-codegen": "graphql-codegen --watch",
    "dev-next": "next dev",
    "dev": "concurrently -k npm:dev-codegen npm:dev-next",
    "prebuild": "graphql-codegen",
    "build": "next build",
    "start": "next start",
    "not-work-lint": "next lint",
    "lint": "eslint --ext .ts,.tsx .",
    "pre-commit": "yarn lint --fix && git add .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,md}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.5.10",
    "@emotion/css": "11.1.3",
    "@emotion/react": "11.4.1",
    "@emotion/styled": "11.3.0",
    "date-fns": "2.28.0",
    "emoji-regex": "10.0.1",
    "gemoji": "^7.1.0",
    "graphql": "16.3.0",
    "mdast-util-find-and-replace": "2.1.0",
    "next": "12.0.10",
    "next-with-apollo": "5.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "8.0.0",
    "react-syntax-highlighter": "15.4.5",
    "rehype-raw": "6.1.1",
    "remark-gemoji": "7.0.1",
    "remark-gfm": "3.0.1"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.7.2",
    "@graphql-codegen/add": "3.1.1",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/near-operation-file-preset": "2.2.8",
    "@graphql-codegen/schema-ast": "2.3.0",
    "@graphql-codegen/typescript": "2.3.1",
    "@graphql-codegen/typescript-operations": "2.2.4",
    "@graphql-codegen/typescript-react-apollo": "3.2.10",
    "@types/node": "17.0.14",
    "@types/react": "17.0.26",
    "@types/react-syntax-highlighter": "13.5.2",
    "@typescript-eslint/eslint-plugin": "4.32.0",
    "@typescript-eslint/parser": "4.32.0",
    "babel-plugin-inline-react-svg": "2.0.1",
    "concurrently": "7.0.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.26.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "7.0.0",
    "lint-staged": "11.1.2",
    "prettier": "2.4.1",
    "typescript": "4.4.3"
  }
}
